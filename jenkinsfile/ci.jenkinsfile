#!groovy
@Library('jenkinslib') _

def build = new org.devops.build()

String buildType = "${env.buildType}"
String buildShell = "${env.buildShell}"
pipeline{
    agent { node {label "master"}}
    stages{
        steps{
            script{
                build.Build(buildType,buildShell)
            }
        }
    }
}
